package com.globussoft;

import java.util.Scanner;
import java.util.Stack;

public class Bracess {
	static int operation1=0;
	static  boolean isBalanced(String st) {

		Stack<Character> stk=new Stack<Character>();
		for(int i=0;i<st.length(); i++) {
			char ch=st.charAt(i);
			if(ch=='{'  ) {
				stk.push(ch);
			}
			else {
				if(stk.isEmpty())
					return false;
				if(ch=='}') {
					operation1++;
					if(stk.pop()!='{')
						return false;
							}
					}
			}
		
		return stk.isEmpty();
	}
	
	static  boolean isBalanced1(String st1) {

		Stack<Character> stk=new Stack<Character>();
		for(int i=0;i<st1.length(); i++) {
			char ch=st1.charAt(i);
			if(ch=='{'  ) {
				stk.push(ch);
			}
			else {
				if(stk.isEmpty())
					return false;
				if(ch=='}') {
					if(stk.pop()!='{')
						return false;
							}
					}
			}
		
		return stk.isEmpty();
	}
	public static void main(String[] args) {
		Scanner ip=new Scanner(System.in);

		System.out.println("Enter the String paranthasis");
		String st=ip.next();
		String st2=ip.next();
		String hf=ip.next();

		boolean rs=isBalanced(st);
		if(rs) {
			System.out.println("1. 0");
		}
		else {
			System.out.println("1."+operation1);
		}
		
		boolean rs2=isBalanced(st2);
		if(rs2) {
			System.out.println("2.0");
		}
		else {
			System.out.println("2.");
		}
	}
}
