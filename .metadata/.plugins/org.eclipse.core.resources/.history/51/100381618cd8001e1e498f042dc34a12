package com.bs18;

import java.util.Scanner;

public class A {


	public static void main(String[] args) {


		Scanner ip = new Scanner(System.in);
		System.out.println("Enter the number");
		
		int num = ip.nextInt();
		
		int rev = 0;
		while(num > 0 ) {
			int lastNum = num % 10;
			rev = (rev * 10) + lastNum;
			num = num / 10;
		}
		
	}

}

