package com.hackerrank;

public class AAAAaaaaaaa {
	
	    public static void main(String[] args) {
	        outerLoop: // This label is used for the outer loop
	        for (int i = 1; i <= 3; i++) {
	            innerLoop:
	            for (int j = 1; j <= 3; j++) {
	                System.out.println("i = " + i + ", j = " + j);

	                if (i == 2 && j == 2) {
	                    // Break out of the inner loop when i is 2 and j is 2
	                    break innerLoop;
	                }
	            }
	        }
	    }
	}

