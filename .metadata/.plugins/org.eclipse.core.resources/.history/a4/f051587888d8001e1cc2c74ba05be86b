package com.bs18;

import java.util.Scanner;

public class A {


	public static void main(String[] args) {


		Scanner ip = new Scanner(System.in);
		System.out.println("Enter the number");
		
		int n = ip.nextInt();
		ip.close();
		for (int i = 0; i < n; i++) {
			if(i % 2 != 0) {
				System.out.println(i);
			}
		}

	}

}