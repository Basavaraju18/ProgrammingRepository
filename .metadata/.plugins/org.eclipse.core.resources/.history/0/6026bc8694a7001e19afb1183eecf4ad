package com.hackerrank;

public class ArrayElementsComparision {

	private static void comaparision() {

		int[] ar1= {12, 10, 9, 13};
		int[] ar2= {10, 15, 81, 18};

		int a=0;
		int b=0;

		for (int i = 0; i < ar1.length; i++) {


			if(ar1[i] > ar2[i]) {
				a++;
			}

			else if(ar1[i] < ar2[i]) {
				b++;
			}

			if(ar1[i] == ar2[i]) {
				a=a+0;
				b=b+0;
			}


		}

		System.out.println(a+" "+ b);
	}

	public static void main(String[] args) {
		comaparision();
	}

}
