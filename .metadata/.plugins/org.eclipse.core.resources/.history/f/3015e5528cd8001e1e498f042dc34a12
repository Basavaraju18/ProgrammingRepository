package com.bs18;

import java.util.Scanner;

public class A {


	public static void main(String[] args) {


		Scanner ip = new Scanner(System.in);
		System.out.println("Enter the number");
		
		int num = ip.nextInt();
		
		int rev = 0;
		while(num>0) {
			int n = num % 10;
			rev = (rev*10)+n;
			num = num/10;
		}
		System.out.println(rev);
	}

}